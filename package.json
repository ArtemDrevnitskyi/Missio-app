    "@supabase/auth-ui-shared": "^0.1.3",
    "@supabase/supabase-js": "^2.20.0",
    "@typebot.io/js": "^0.0.43",
    "@typebot.io/react": "^0.0.43",
    "formik": "^2.2.9",
    "framer-motion": "^8.5.5",
